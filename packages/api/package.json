{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc && mkdir -p dist/services/pdf && cp -r src/services/pdf/assets dist/services/pdf/",
    "dev": "tsc --watch",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@trpc/server": "^11.5.1",
    "db": "workspace:^",
    "decimal.js": "^10.4.3",
    "lib": "workspace:^",
    "jspdf": "^2.5.1",
    "typescript": "^5.3.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.0.0",
    "drizzle-orm": "^0.28.6",
    "vitest": "^1.0.0"
  }
}